    android:insetBottom="-4dp"
    android:insetLeft="0dp"
    android:insetRight="0dp">
    <shape android:shape="rectangle">
        <solid android:color="@android:color/white" />
        <corners android:radius="10dp" />
        <stroke
            android:width="1dp"
            android:color="#9B59B6" />
    </shape>
</inset>
